#include<stdio.h>

int main(){
int num,temp,rem,sum=0;

printf("Enter the number: ");

scanf("%d",&num);

num = temp;

while(temp!=0){
rem = temp % 10 ;
sum = sum * 10 + rem;
temp = temp / 10;
}
if(num==sum){
printf("The number is armstrong number");
}
else{
printf("The number is not a armstrong number")
}
return 0;
}
